{
  "name": "gorilla-vault-api",
  "version": "0.0.1",
  "description": "This is gorilla vault secure storage solution backend service",
  "author": "Giorgi Kumelashvili",
  "private": true,
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.13.0 <17.0.0"
  },
  "scripts": {
    "check:update": "npx npm-check-updates",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:e2e": "jest --config ./test/e2e/jest.json --detectOpenHandles --forceExit",
    "test:e2e:cov": "jest --config ./test/e2e/jest.json --coverage --detectOpenHandles --forceExit",
    "test:unit": "jest --config ./test/unit/jest.json --detectOpenHandles --forceExit",
    "test:unit:cov": "jest --config ./test/unit/jest.json --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:gen": "npx prisma generate",
    "migration:run": "npx prisma migrate dev"
  },
  "dependencies": {
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/platform-express": "^10.0.5",
    "@prisma/client": "^5.1.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.1",
    "npm": "^9.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.1",
    "ts-pattern": "^5.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.8",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.1",
    "prettier": "^3.0.0",
    "prisma": "^5.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "testcontainers": "^9.10.1",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  }
}
