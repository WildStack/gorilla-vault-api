{
  "name": "gorilla-vault-api",
  "version": "0.0.1",
  "description": "This is gorilla vault secure storage solution backend service",
  "author": "Giorgi Kumelashvili",
  "private": "false",
  "license": "MIT",
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "use:nvm": "source ~/.nvm/nvm.sh && nvm use",
    "install": "npm run use:nvm",
    "preinstall": "npm run use:nvm",
    "preversion": "npm run use:nvm",
    "prestart": "npm run use:nvm",
    "prebuild": "npm run use:nvm && rimraf dist",
    "prisma": "prisma",
    "modules:update:check": "npm run use:nvm && npx npm-check-updates",
    "modules:update": "npm run use:nvm && npx npm-check-updates -u",
    "build": "npm run lint && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run use:nvm && nest start",
    "start:dev": "npm run use:nvm && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:e2e": "jest --config ./test/e2e/jest.json --detectOpenHandles --forceExit",
    "test:e2e:cov": "jest --config ./test/e2e/jest.json --coverage --detectOpenHandles --forceExit",
    "test:unit": "jest --config ./test/unit/jest.json --detectOpenHandles --forceExit",
    "test:unit:cov": "jest --config ./test/unit/jest.json --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:prisma:gen": "prisma generate",
    "migration:gen": "prisma migrate dev --create-only",
    "migration:run": "prisma migrate dev"
  },
  "dependencies": {
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.2.10",
    "@prisma/client": "^5.12.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "figlet": "^1.7.0",
    "helmet": "^7.1.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^9.3.0",
    "moment": "^2.29.4",
    "npm": "^10.2.5",
    "nunjucks": "^3.2.4",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sanitize-filename": "^1.6.3",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.7.2",
    "ts-pattern": "^5.0.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@testcontainers/postgresql": "^10.8.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/figlet": "^1.5.8",
    "@types/jest": "29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/nunjucks": "^3.2.6",
    "@types/sanitize-html": "^2.11.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.12.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "testcontainers": "^10.3.2",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  }
}
